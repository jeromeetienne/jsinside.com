---
layout: default
---

<div class="container-fluid row">
	<h1><%= document.title %></h1>
	<div class='col-md-8'>
		<iframe width="100%" height="350"  frameborder="0" allowfullscreen
			src="//www.youtube.com/embed/<%= document.youtubeId%>">
		</iframe>
	</div>
	<div class='col-md-4'>
		<div><small><%= document.date.toDateString() %></small></div>
		<div><%- content %></div>
		<p>
			<a href="/data/<%= document.name %>/slides/" class="btn btn-primary" role="button" target='_blank'>Slides</a>
		</p>
	</div>
</div>



<footer>
	<% var relatedDocs	= getRelatedDocuments(); %>
	<% relatedDocs		= relatedDocs.filter(function(document){
		return document.meta.layout === 'course' ? true : false
	}) %>
	<% if( relatedDocs.length ){ %>
		<section id="related">
			<h3>Related Courses</h3>
			<nav class="linklist">
				<% relatedDocs.forEach(function(document){ %>
					<li>
						<a href="<%= document.url %>"><%= document.title %></a>
						<!-- <pre><%= JSON.stringify(document, null, '\t') %></pre> -->
					</li>
				<% }) %>
			</nav>
		</section>
	<% } %>
</footer>

